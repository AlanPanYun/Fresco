<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="alanpan.gbi.com.frescodemo">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".switchlangueapp.SwitchLanguageMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FrescoActivity" />
        <activity android:name=".PicassoActivity" />
        <activity android:name=".switchlangueapp.SecondActivity"/>
        <activity android:name=".switchlangueapp.SwitchLanguageBaseActivity"/>
        <activity android:name=".customview.CustomViewActiivty"/>
        <activity android:name=".databinding.DataBindingActivity"/>

    </application>

</manifest>